{
  "name": "cf_ai_coderoom",
  "version": "0.1.0",
  "private": true,
  "description": "Realtime pair programming assistant built on Cloudflare",
  "workspaces": [
    "frontend",
    "worker"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:worker\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:worker": "npm run dev --workspace=worker",
    "build": "npm run build --workspace=frontend && npm run build --workspace=worker",
    "build:frontend": "npm run build --workspace=frontend",
    "build:worker": "npm run build --workspace=worker",
    "deploy": "npm run deploy:worker && npm run deploy:frontend",
    "deploy:frontend": "npm run deploy --workspace=frontend",
    "deploy:worker": "npm run deploy --workspace=worker",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "pair-programming",
    "durable-objects"
  ],
  "author": "",
  "license": "MIT"
}
